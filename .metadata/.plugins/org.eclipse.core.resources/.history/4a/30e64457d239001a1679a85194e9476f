package InterviewPrograms;

public class PatternPrograms {

	
	
	
	
	
	
	public static void Squer(){
		
		for(int i=1;i<=5;i++){
			
			
			for(int j=5;j>=i;j--){
				System.out.print(i);
				
				
			}
			
			System.out.println("\n");
		}
		
		
		
	}
	
	
	public static void main(String[] args) {
		
		
		Squer();
	}
	
	
	
	
	
	
	
}
